{
    "configurations": [
        {
            "name": "Mac",
            "compilerPath": "/opt/homebrew/bin/arm-none-eabi-gcc",
            "includePath": [
                "${workspaceFolder}/API/*",
                "${workspaceFolder}/API/cxxsupport/*",
                "${workspaceFolder}/API/Inc/*",
                "${workspaceFolder}/API/Src/*",                
                "${workspaceFolder}/Degree/*",
                "${workspaceFolder}/Degree/Inc/*",
                "${workspaceFolder}/Degree/Src/*",
                "${workspaceFolder}/Drivers/*",
                "${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/*",
                "${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Inc/*",
                "${workspaceFolder}/Drivers/STM32F4xx_HAL_Driver/Src/*",
                "${workspaceFolder}/ok-drivers/**",
                "${workspaceFolder}/System/*",
                "${workspaceFolder}/System/Inc/*",
                "${workspaceFolder}/System/Src/*"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${default}",
                "path": [
                    "${workspaceFolder}/**",
                    ""
                ]
            },
            "intelliSenseMode": "macos-gcc-arm",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14"
        }
    ],
    "version": 4
}